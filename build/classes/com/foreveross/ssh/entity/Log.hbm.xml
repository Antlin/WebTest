<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.foreveross.ssh.entity">
		<class name="Log" table="tb_log">
			<id name="logId" column="ID">
				<generator class="native"></generator>
			</id>
			<property name="logType" column="type"/>
			<property name="logDate" column="time"/>
		</class>
	</hibernate-mapping>